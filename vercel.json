{
  "version": 2,
  "builds": [
    {
      "src": "./package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./(.*)",
      "dest": "./server.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' https://vercel.live"
        }
      ]
    }
  ]
}
